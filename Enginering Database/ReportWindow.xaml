<Window x:Class="Engineering_Database.ReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Engineering_Database"
        mc:Ignorable="d"
        Title="Reports" Height="450" Width="800">
    <Grid>
        <Button Name="ReportsForJobs" Content="Reports For Jobs" HorizontalAlignment="Left" Margin="20,23,0,0" VerticalAlignment="Top" Width="123" Height="31" Click="ReportsForJobs_Click"/>
        <Button x:Name="AssetReportButton" Content="Report For Asset" HorizontalAlignment="Left" Margin="20,59,0,0" VerticalAlignment="Top" Width="123" Height="31" Click="AssetReportButton_Click"/>
        <TextBox Name="AssetTextBox" HorizontalAlignment="Left" Height="31" Margin="165,59,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="ChartButton" Content="Chart" HorizontalAlignment="Left" Margin="20,95,0,0" VerticalAlignment="Top" Width="123" Height="31" Click="ChartButton_Click"/>

    </Grid>
</Window>
