<Window x:Class="Engineering_Database.passwordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Engineering_Database"
        mc:Ignorable="d"
        Title="Please Enter Password" Height="134.895" Width="368.97">
    <Grid>
        <PasswordBox x:Name="passwordWindowTextBox" HorizontalAlignment="Center" Margin="68,36,67,0" VerticalAlignment="Top" Width="200" />
        <Button Content="Submit" HorizontalAlignment="Center" Margin="142,68,149,0" VerticalAlignment="Top" Width="70" Click="Button_Click"/>
        <Label x:Name="passwordMessage" Content="Damn... password is wrong! Try again!" HorizontalAlignment="Left" Margin="35,10,0,0" VerticalAlignment="Top" Width="284" HorizontalContentAlignment="Center"/>

    </Grid>
</Window>
