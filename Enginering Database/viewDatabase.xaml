        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Enginering_Database"
        mc:Ignorable="d"
        Title="View Database" Height="{Binding SystemParameters.PrimaryScreenHeight}" Width="{Binding SystemParameters.PrimaryScreenWidth}" ResizeMode="NoResize" 
       >
    <Grid Margin="0,0,0,-21">
        <Grid.RowDefinitions>
            <RowDefinition Height="212*"/>
            <RowDefinition Height="207*"/>
        </Grid.RowDefinitions>
        <DataGrid x:Name="excelDataGridData" Margin="10,0,10,41" Grid.RowSpan="2"/>
        <Grid HorizontalAlignment="Left" Height="399" Margin="10,0,0,0" Grid.RowSpan="2" VerticalAlignment="Top" Width="772"/>
    </Grid>
</Window>
